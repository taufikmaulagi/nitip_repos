.table-hover.table-bordered tbody tr.checked.hover td, .table-hover.table-bordered tbody tr.checked.hover th {
	border-top: 0 none;
	border-right: .0625rem solid #20B9CE !important;
	border-left: .0625rem solid #20B9CE !important;
	color: #fff !important;
}
.table-hover tbody tr.not-hover:hover td, .table-hover tbody tr.not-hover:hover th {
